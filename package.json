{
  "name": "writting-sass",
  "version": "1.0.0",
  "description": "Compilation du code scss en css plus les préfixes propres à chaque navigateur",
  "main": "index.js",
  "scripts": {
    "sass": "sass --watch ./sass/main.scss:./public/css/style.css",
    "prefix": "postcss ./public/css/style.css --use autoprefixer -d ./public/css/prefixed/"
  },
